# Local development override example
# Copy this file to docker-compose.override.yml for local development
version: '3.8'

services:
  invoice-app:
    environment:
      - ACTIVE_MQ_BROKER_URL=tcp://activemq:61616
    volumes:
      - ./logs:/opt/payara/glassfish/domains/domain1/logs
      - ./invoice-approval-web/target:/opt/payara/deployments
